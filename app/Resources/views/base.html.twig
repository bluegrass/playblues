<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        
        {% block stylesheets %}
        
            <link rel="stylesheet" href="{{ asset('assetic/bluegrassbluesbundle_stylesheets.css') }}" type="text/css" />

        {% endblock %}
        
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}{% endblock %}
            
        {% block javascripts %}
            
            <script type='text/javascript' src="{{ asset('js/jquery-1.8.0.min.js') }}"></script>
            
            <script src="{{ asset('assetic/bluegrassbluesbundle_javascripts.js') }}" type="text/javascript"></script>
            
            {{ viewstate_render(viewState) }}
            
        {% endblock %}
    </body>
</html>
